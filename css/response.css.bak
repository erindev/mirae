/* response-pc */
	@media  all  and  (min-width:1281px) {}
/* response-tablet */
	@media  all  and  (min-width:768px) and  (max-width:1280px)   {
	#header .container {width:90%;}
	#visual .visual_banner img {margin-left:-50%;}
	#main {}
	.m1 .container {width:90%;}
	.m1 ul {margin-left:-25px;height:150px; padding-top:10px; margin-left:-5px;}
	.m1 ul li.a51, .m1 ul li.a52, .m1 ul li.a53 {width:33.3%;}
	.m1 ul li.a53 {border-right:0; width:32.3%;}
	.m1 ul li.a54 {display:none;}
	.m1 ul li.a55 {display:none;}
	
	.m2 .container {width:90%;}
	.m2 article.a31 {margin-left:-4px;}
	.m2 article.a31, .m2 article.a32, .m2 article.a33 {width:33.3%;}
	.m2 article a {width:90%;}
	.m2 article.a31 ul {width:100%;}
	.m2 article.a31 img, .m2 article.a32 img {width:90%;}
	

	.m3 .container {width:90%;}
	.m3 article.a31 {margin-left:-4px;}
	.m3 article a, .m3 article p, .m3 article.a31 ul li span {width:80%;}
	.m3 article li {font-size:15px;}
	.m3 article.a31, .m3 article.a32, .m3 article.a33 {width:33.3%;}
	.m3 article.a32 {overflow:hidden;}
	.m3 article.a32 img {width:90%;}
	.m3 .a32 h4 {float:left;}
	.m3 .a33 p {text-align:center;}
	.m3 article.a33 img {width:100%; margin-top: 20px;}
	/* .m3 .a31 ul li {font-size:14px;} */
	.m3 .a31 ul li span.response_hidden {font-size:13px; text-align:center;}
	.m3 .a31 ul li {margin-right:1px;}
	
	.mf1 .container {width:90%;}
	.mf1 div.a3 {width:33.3%;}
	.mf1 div.a3 dl {height: 60px; width:50%; margin-left:25%;}
	.mf1 div.a3 dt {width:100%; text-align:left; margin-right:-20px;}

	.mf2 {height:540px;}
	.mf2 .container {width:90%;}
	.mf2 div.a3 dl {height:45%;}
	.mf2 div.a3 dl.l3 dd a {height:25px;}
	.mf2 .w50 .l1 {height:35%;}
	.mf2 .w50 .l2 {height:40%;}
	.mf2 .w50 .l3 {height:23%;}
	.mf2 .w25 .l1 {height:30%;}
	.mf2 .w25 .l2 {height:30%;}
	.tiger {display:none;}

	.mf3 {height:80px;}
	.mf3 .container {width:90%;}
	}
/* response-mobile */
	@media  all  and     (max-width:767px) {
	#header {height:100px;}
	#header .container {width:90%;}
	.mobile_menu {display:block;}
	.mobile_menu img { width:25px; height:25px; margin-top:35px;}
	ul.lnb li img {width:38px; height:38px;}
	ul.lnb .lnb_img {width:38px; height:38px;}
	ul.lnb li.lnb_img {margin-top:30px;}
	#header h1.logo {top:10%; left:35%;}
	#header ul {height:100%;}
	#header ul.lnb {margin-top:-65px;}
	#header ul.lnb li.lnb1, #header ul.lnb li.lnb2 {display:none;}
		/* #header ul li.lnb_img a {background-size:;} */
	#header nav {display:none;}

	#visual {margin-top:80px; }
	#visual ul.visual_banner {height:50%;}
	#visual ul.visual_banner li {}
	#visual ul.visual_banner img {width:200%; margin-left:-100px;}
	#main {}
	.m1 .container {width:90%;}
	.m1 {height: 300px;}
	.m1 h3 {text-indent:0em;}
	.m1 h4 {display:block;}
	.m1 ul {height:60%;}
	.m1 ul li strong {text-align:left; color:#ff6600; font-size:16px;}
	.m1 ul li span {display:block; float:left; width:70%; text-align:justify; text-overflow: ellipsis; overflow:hidden;}
	.m1 ul li img {float:right; width:20%;}
	.m1 ul li.a51 {width:100%; border-right:0;}
	.m1 ul li.a52, .m1 ul li.a53, .m1 ul li.a54,.m1 ul li.a55 {display:none;}
	.m1 ul.a5.circle_btn {display:block;}
	
	.m2 {display:none;}

	.m_content.m4 {display:block; margin-top:10px;}
	.m_content.m4 article {float:left; width:45%;}
	.m_content.m4 .response1 {border-right:1px solid #eee;}
	.m_content.m4 article {padding:5px 5px; overflow:hidden;}
	.m_content.m4 img {width:90%; margin:auto;}
	.m_content.m4 h3 {font-size:14px; color:#ff3300; float:left;}
	.m_content.m4 p.date {float:right;}
	.m_content.m4 h4 {float:left; font-size:13px; color:#000;}



	/* .m3 완성 */
	.m3 {height:100px; border-bottom:1px solid #eee;}
	.m3 .container {width:90%;}
	.m3 .a3 h3 {margin: 10px 0; text-indent:0em;}
	.m3 .a31 ul {margin-left:0; width:33%; height: 50%;}
	.m3 .a31_3 {border-right:0 none;}
	.m3 .a31 ul {border-right:1px solid #eee;}
	.m3 .a31 ul li {margin-right:0px; width:100%;}
	.m3 article a, .m3 article p, .m3 article.a31 ul li span {width:80%;}
	article.a31 ul {float:left;}
	.m3 article li {font-size:11px; text-indent:0.5em;}
	.m3 article.a31 {width:100%; border-right:0 none;}
	.m3 .a31 p, .m3 .response_hidden, .m3 article.a32, .m3 article.a33 {display:none;}
	.m3 .a31_2, .m3 .a31_3 {display:block;}



	/* footer 완성 */
	.mf1 {height:84px;}
	.mf1 .container {width:90%;}
	.mf1 h4 {display:none;}
	.mf1 div.a33 {width:100%; height:84px; }
	.mf1 div.a33 dl {width:100%; height:100%; margin-top:0px; overflow:visible;}
	.mf1 div.a33 dd {width:18%; height:100%;}
	.mf1 div.a33 dd a {display:block; width:100%; height:100%; text-align:center;}
	.mf1 div.a33 dd img {width:90%; height:90%;}
	.mf1 div.a31, .mf1 div.a32, .mf1 div.a33 dt {display:none;}

	.mf2 {display:none;}
	.mf3 {display:none;}

	}	
